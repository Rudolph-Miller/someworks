!!!
%html
  %head
    %title Someworks
    = stylesheet_link_tag    'application', media: 'all', :debug => true
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    .container
      .row
        .col-md-10
          = image_tag "img_sitelogo.png", style: "width:160px; height:35px;margin-left: 20px; margin: 15px;"
        .col-md-2
          = link_to "Logout", destroy_publisher_session_path, :method => :post
    = yield
